{
  "name": "desktop-calendar",
  "version": "1.0.0",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "set BROWSER=none && react-scripts start",
    "start-with-browser": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently --kill-others --names \"REACT,ELECTRON\" -c \"blue,green\" \"cross-env PORT=3002 BROWSER=none npm run start\" \"wait-on http://localhost:3002 && electron .\"",
    "electron-dev-light": "cross-env NODE_ENV=development PORT=3002 BROWSER=none react-scripts start & wait-on http://localhost:3002 && electron .",
    "start-minimal": "cross-env NODE_ENV=development PORT=3002 BROWSER=none GENERATE_SOURCEMAP=false FAST_REFRESH=false react-scripts start",
    "dev-fixed": "start /B cross-env PORT=3002 BROWSER=none GENERATE_SOURCEMAP=false npm run start && wait-on http://localhost:3002 && electron .",
    "electron-only": "electron .",
    "build-electron": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never"
  },
  "keywords": [
    "calendar",
    "desktop",
    "electron",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "description": "A beautiful desktop calendar application",
  "dependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "ajv": "^8.17.1",
    "antd": "^5.27.0",
    "better-sqlite3": "^12.2.0",
    "dayjs": "^1.11.13",
    "electron-is-dev": "^3.0.1",
    "react": "^18.3.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.2",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/react-big-calendar": "^1.16.2",
    "@types/styled-components": "^5.1.34",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "wait-on": "^8.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
